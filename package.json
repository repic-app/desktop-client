{
  "name": "fire-alert-electron",
  "version": "1.0.0",
  "main": "./main/index.js",
  "author": "Margox",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --config ./build/webpack.development.js -d --history-api-fallback --no-inline --progress --colors --hot",
    "start": "cross-env NODE_ENV=development electron ./main/ --debug",
    "start-prod": "cross-env NODE_ENV=production electron ./dist/main/ --debug",
    "build": "cross-env rm -rf ./dist && mkdir dist && cp ./package-dist.json ./dist/package.json && cp -rf ./main ./dist/main && cross-env NODE_ENV=production webpack --config ./build/webpack.production.js --progress --colors",
    "package": "npm run build && electron-packager ./dist --out ./dist-apps --appname 'Firing!' --asar --electronVersion 4.1.2 --overwrite --platform darwin,linux",
    "package-win": "npm run build && electron-packager ./dist --out ./dist-apps --appname 'Firing!' --asar --electronVersion 4.1.2 --overwrite --platform win32",
    "lint": "node_modules/eslint/bin/eslint.js ."
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@hot-loader/react-dom": "16.8.5",
    "dayjs": "^1.8.12",
    "file-encryptor": "^0.1.1",
    "imagemin": "^6.1.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-pngquant": "^7.0.0",
    "imagemin-svgo": "^7.0.0",
    "js-base64": "^2.5.1",
    "query-string": "^6.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "rematch": "^0.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "babel-plugin-import": "^1.9.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "cross-env": "^5.2.0",
    "css-hot-loader": "^1.4.2",
    "css-loader": "^0.28.11",
    "eslint": "^5.4.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "extract-text-webpack-plugin": "next",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "react-hot-loader": "^4.3.11",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-bundle-analyzer": "^3.0.2",
